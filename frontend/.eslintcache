  }\r\n\r\n    return (\r\n        <Container fluid>\r\n            <Row className=\"justify-content-center my-3\">\r\n                <Button className=\"text-center\" variant=\"primary\" onClick={modalHandler}>New Post</Button>\r\n            </Row>\r\n            <Modal\r\n                ariaHideApp={false}\r\n                style={customStyles}\r\n                isOpen={modal}\r\n                onRequestClose={modalHandler}\r\n            >\r\n                <Form onSubmit={submitHandler}>\r\n                    <Form.Group controlId=\"formPictureUpload\">\r\n                        <Form.Label>Picture Caption</Form.Label>\r\n                        <Form.Control\r\n                            type=\"caption\"\r\n                            placeholder=\"Enter a Caption\"\r\n                            value={caption}\r\n                            onChange={(e) => setCaption(e.target.value)} />\r\n                    </Form.Group>\r\n\r\n                    <Form.Group controlId='picture'>\r\n                        <Form.Label>Picture </Form.Label>\r\n                        <Form.Control\r\n                            type='text'\r\n                            placeholder='Enter Image url'\r\n                            value={image}\r\n                            onChange={(e) => setImage(e.target.value)}>\r\n                        </Form.Control>\r\n                        <Form.File\r\n                            id='image-file'\r\n                            label='Choose file'\r\n                            custom\r\n                            onChange={uploadFileHandler}></Form.File>\r\n                    </Form.Group>\r\n\r\n                    <Button variant=\"primary\" type=\"submit\">\r\n                        Submit\r\n                    </Button>\r\n\r\n                </Form>\r\n            </Modal>\r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default FileUploader\r\n","C:\\Users\\charlie\\Desktop\\social-media\\frontend\\src\\s