lPost(true)\r\n        }\r\n\r\n    }, [success])\r\n\r\n    return (\r\n        <div className=\"post\" >\r\n            <div className=\"post__header\">\r\n                <div className=\"post__header__left\">\r\n                    <Link className=\"link\" to={`/${post.user.username}`}>\r\n                        <Avatar\r\n                            className=\"post__avatar\"\r\n                            alt={post.user.username}\r\n                            src={post.user.profileImg}>\r\n                        </Avatar>\r\n                    </Link>\r\n                    <Link className=\"link\" to={`/${post.user.username}`}><h3>{post.user.username}</h3></Link>\r\n                </div>\r\n\r\n                <Dropdown className='post__header__right' >\r\n                    <Dropdown.Toggle as={CustomToggle} id=\"dropdown-custom-components\">\r\n\r\n                    </Dropdown.Toggle>\r\n\r\n                    <Dropdown.Menu>\r\n                        <Dropdown.Item onClick={() => deletePostHandler(post._id)} eventKey=\"1\">Delete Post</Dropdown.Item>\r\n                        <Dropdown.Item eventKey=\"2\">Report</Dropdown.Item>\r\n                    </Dropdown.Menu>\r\n                </Dropdown>\r\n            </div>\r\n            {/*Image*/}\r\n            <img\r\n                className=\"post__image\"\r\n                src={post.image}\r\n            >\r\n            </img>\r\n\r\n\r\n            {/*Username + Caption*/}\r\n            {post.caption &&\r\n                <h5 className=\"post__text\"><Link\r\n                    to={`/${post.user.username}`} className=\"link\"><strong>{post.user.username}</strong></Link>{' '}{post.caption}</h5>}\r\n\r\n            {/*Comments*/}\r\n            {post.comments.map(x => (\r\n                <div className=\"post__text__comment\" key={x._id}>\r\n                    <div className=\"post__comment\">\r\n                        <Link className=\"link\" to={`/${x.username}`}><strong>{x.username}</strong></Link>{' '}{x.comment}\r\n                    </div>\r\n                    <div className=\"mr-2\">\r\n                        {!delPost && <Link onClick={(() => {\r\n                            deleteComment(post._id, x._id)\r\n                        })}><TiDeleteOutline color=\"light-gray\" /></Link>}\r\n                    </div>\r\n                </div>\r\n            ))}\r\n\r\n            {loadingPost === false ? (\r\n                <div className=\"form\">\r\n                    <form onSubmit={commentHandler} >\r\n                        <div className=\"form__input\">\r\n                            <input\r\n                                type=\"text\"\r\n                                placeholder=\"Comment\"\r\n                                value={comment} onChange={(e) => setComment(e.target.value)} />\r\n                        </div>\r\n                        <div className=\"form__button\">\r\n                            <button\r\n                                type='submit'\r\n                                variant='primary'>\r\n                                Submit\r\n                        </button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            ) : (\r\n                <div className=\"icon\"><a className=\"link\" href={`/post/${post._id}`}><BiCommentDetail size=\"30px\" /> Add a comment</a>\r\n                </div>\r\n            )}\r\n\r\n        </div >\r\n    )\r\n}\r\n\r\nexport default Post\r\n\r\n","C:\\Users\\Charlie\\Desktop\\social-media\\frontend\\src\\index.js",[],["230","231"],"C:\\Users\\Charlie\\Desktop\\social-media\\frontend\\src\\store.js",[],"C:\\Users\\Charlie\\Desktop\\social-media\\frontend\\src\\App.js",[],"C:\\Users\\Charlie\\Desktop\\social-media\\frontend\\src\\reducers\\UserReducers.js",[],"C:\\Users\\Charlie\\Desktop\\social-media\\frontend\\src\\screens\\SignupScreen.js",["232","233"],"import React, { useState, useEffect } from 'react'\r